<link rel="stylesheet" href="/css/matricula/agregar.css">
{{#each list}}
<form action="/cars/matricula/add/{{idCars}}" method="post">
    <div id="datosCarro">
        <div id="marca">
            <label>Marca:</label>
            <p>{{brandCars}}</p>
        </div>
        <div id="clase">
            <label>Clase:</label>
            <p>{{classCars}}</p>
        </div>
        <div id="año">
            <label>Año:</label>
            <p>{{yearsCars}}</p>
        </div>
        <div id="placa">
            <label>Placa:</label>
            <p>{{licenseplateCars}}</p>
        </div>
        <div id="modelo">
            <label>Modelo:</label>
            <p>{{modelCars}}</p>
        </div>
        <div id="motor">
            <label>Motor:</label>
            <p>{{engineCars}}</p>
        </div>
        <div id="chasis">
            <label>Chasis:</label>
            <p>{{chassisCars}}</p>
        </div>
    </div>
{{/each}}
    <div id="numeroDeColumnas">
        <label>Columnas Necesarias:</label>
        <input type="text" name="columnas" id="columnas">
        <button type="button" onclick="tabla.generarTablas()">Generar</button>
    </div>
    <div id="formulario"></div>
</form>

<script src="/js/tabla/tabla.js"></script>