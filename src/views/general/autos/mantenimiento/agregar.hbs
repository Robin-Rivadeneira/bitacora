<link rel="stylesheet" href="/css/mantenimiento/agregar.css">
{{#each list}}
<form action="/cars/mantenimiento/add/{{idCars}}" method="post">
    <div id="datosCarro">
        <div id="marca">
            <label>Marca:</label>
            <p>{{brandCars}}</p>
        </div>
        <div id="clase">
            <label>Clase:</label>
            <p>{{classCars}}</p>
        </div>
        <div id="año">
            <label>Año:</label>
            <p>{{yearsCars}}</p>
        </div>
        <div id="placa">
            <label>Placa:</label>
            <p>{{licenseplateCars}}</p>
        </div>
        <div id="modelo">
            <label>Modelo:</label>
            <p>{{modelCars}}</p>
        </div>
        <div id="motor">
            <label>Motor:</label>
            <p>{{engineCars}}</p>
        </div>
        <div id="chasis">
            <label>Chasis:</label>
            <p>{{chassisCars}}</p>
        </div>
          <div id="responsable">
            <label>RESPONSABLE DEL VEHICULO:</label>
            <input type="text" name="responsable" id="">
        </div>
    </div>
    {{/each}}
    <div id="Fecha">
        <label>Semana:del<input type="date" name="" id=""> al <input type="date"></label>
    </div>
    <div id="fechaEntrega">
        <label>Fecha de Entrega: <input type="date" name="fehcaEntrega" id=""></label>
    </div>
    <div id="FIRMA">
        <label>Firma de responsab:</label>
    </div>
    <div id="numeroDeColumnas">
        <label>Columnas Necesarias:</label>
        <input type="text" name="columnas" id="columnas">
        <button type="button" onclick="tabla.generarTablas()">Generar</button>
    </div>
    <div id="mantenimiento">
    </div> 
</form>

<script src="/js/tabla/mantenimiento.js"></script>